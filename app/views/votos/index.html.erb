<p id="notice"><%= notice %></p>

<h1>Votos</h1>

<table>
  <thead>
    <tr>
      <th>Eleitor</th>
      <th>Int</th>
      <th>References,</th>
      <th>Candidato</th>
      <th>Int</th>
      <th>References</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votos.each do |voto| %>
      <tr>
        <td><%= voto.eleitor_id %></td>
        <td><%= voto.int %></td>
        <td><%= voto.references, %></td>
        <td><%= voto.candidato_id %></td>
        <td><%= voto.int %></td>
        <td><%= voto.references %></td>
        <td><%= link_to 'Show', voto %></td>
        <td><%= link_to 'Edit', edit_voto_path(voto) %></td>
        <td><%= link_to 'Destroy', voto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voto', new_voto_path %>
